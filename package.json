    "@wordpress/block-library": "^2.26.4",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^0.8.0",
    "cache-manager-fs-hash": "^0.0.9",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-formatjs": "^2.9.10",
    "gatsby": "^2.27.0",
    "gatsby-image": "^2.6.0",
    "gatsby-image-graphql-schema": "^1.1.1",
    "gatsby-plugin-create-client-paths": "^2.5.0",
    "gatsby-plugin-graphql-loader": "^1.0.2",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-plugin-styled-components": "^3.6.0",
    "gatsby-plugin-webfonts": "^1.1.3",
